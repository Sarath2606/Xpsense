{
  "name": "xpenses-backend",
  "version": "1.0.0",
  "description": "Backend for Xpenses Open Banking Integration",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node scripts/migrate-database.js",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "db:cleanup": "node cleanup-test-data.js",
    "db:reset-all": "node reset-database.js",
    "db:check": "node check-accounts.js",
    "test:mastercard": "jest --testPathPattern=mastercard",
    "test:consent": "jest --testPathPattern=consent",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "dev:rate-limit-fix": "node restart-with-new-rate-limits.js",
    "dev:firebase-auth": "node restart-with-firebase-auth.js",
    "setup:email-invitations": "node setup-email-invitations.js"
  },
  "keywords": [
    "expenses",
    "open-banking",
    "mastercard",
    "finance"
  ],
  "author": "Xpenses Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "@types/nodemailer": "^7.0.1",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^13.4.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.6",
    "prisma": "^5.7.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
